<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="hms_doctor_form_view" model="ir.ui.view">
            <field name="name">Doctor Form View</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="f_name"/>
                            <field name="l_name"/>
                            <field name="image"/>
                        </group>
                    </sheet>
                </form>
            </field>

        </record>
        <record id="hms_doctor_tree_view" model="ir.ui.view">
            <field name="name">Doctor Tree View</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="f_name"/>
                    <field name="l_name"/>
                    <field name="image"/>
                </tree>
            </field>
        </record>

        <record id="doctor_action" model="ir.actions.act_window">
            <field name="name">Doctors</field>
            <field name="res_model">hms.doctor</field>
        </record>
        <menuitem id="hms_doctor_menu" name="Doctor" parent="hms_root" action="doctor_action"/>
    </data>
</odoo>